<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.edu.gxnu.mapper.RoleMapper">
    <select id="getRole" resultType="roles" >
        SELECT * FROM roles where rolesId=#{rolesId,jdbcType=VARCHAR}
    </select>

    <select id="viewRole" resultType="roles" >
        SELECT * FROM Roles
    </select>
</mapper>